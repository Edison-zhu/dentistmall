	<script>  
      $.supper('setbasepath'); 
      $(function(){  
	      $('.scrollbarlist').slimScroll({
		        height: $(window).height()-120+'px', 
		        wheelStep:'5px'
		    });
		   $('.scrollbarlist2').slimScroll({
		        height: $(window).height()-180+'px', 
		        wheelStep:'5px'
		    });
			$('.scrollbarinfo').slimScroll({
		        height: $(document.body).height()-50,
		         wheelStep:'5px'
		    });
		    
		    $('.scrollbarinfo2').slimScroll({
		        height: 600,
		         wheelStep:'5px'
		    });
		    
		    $('.scrollbarinfo3').slimScroll({
		        height: 440,
		         wheelStep:'5px'
		    });
		    
		    $('.scrollbarUpdateinfo').slimScroll({
		        height: $(document.body).height()-180,
		         wheelStep:'5px'
		    });
		    
		    checkControl();
		     $.supper("setDownKey");
	    });
	    function checkControl(){
	    	var UserControlList = $.supper("getProductArray", "UserControlList");
	    	$("body *").each(function(i,v){
				var sconCode=$(v).attr("sconCode");
				if(sconCode != null && sconCode != ""){
					var ifOperate=0;
					if(UserControlList != null && UserControlList.length >0){
						for(var i =0 ; i < UserControlList.length;i++){
							if(sconCode==UserControlList[i].SCON_CODE){
								ifOperate=UserControlList[i].IF_OPERATE;
								break;
							}
						}
					}
					if(ifOperate==0)
						$(v).hide();
					else if(ifOperate==1)
						$(v).show();
					else if(ifOperate==2){
						$(v).show();
						$(v).attr("disabled","disabled");
					}
				}
			});
	    }
</script> 